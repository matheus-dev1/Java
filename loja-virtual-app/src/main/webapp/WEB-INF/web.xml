<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_2_4.xsd" id="WebApp_ID" version="2.4">
  <display-name>loja-virtual-app</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>listaCds</servlet-name>
  	<servlet-class>br.com.devmedia.lojavirtual.ListaCdsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>listaCds</servlet-name>
  	<url-pattern>/listacds</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>confirmaDadosServlet</servlet-name>
  	<servlet-class>br.com.devmedia.lojavirtual.ConfirmaDadosServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ConfirmaDadosServlet</servlet-name>
  	<url-pattern>/confirmaDadosServlet</url-pattern>
  </servlet-mapping>
</web-app>